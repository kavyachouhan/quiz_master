{
    "version": 2,
    "builds": [
        {
            "src": "quiz_master/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "includeFiles": ["quiz_master/**/*"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "quiz_master/app.py"
        }
    ]
}